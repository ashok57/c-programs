#include<stdio.h>
int add(int, int);
int main(){
    int result;
    result = add(10,20);
    printf("the result is %d ",result);
    return 0;
}
int add(int a, int b){
    int temp;
    temp =  a + b;
    return temp;
}

// WHEN TEMP VALUE IS VARIABLE :IE IT CHANGES ACCORDING TO THE VALUES ENTERD
#include<stdio.h>
int add(int, int);
int main(){
    int result;
    result = add(10,-20);
    printf("the result is %d ",result);
    return 0;
}
int add(int a, int b){
    int temp;
    temp =  a + b;
    if(temp<0){
        return 0;
    }
    else{
        return temp;
        
    }
}
