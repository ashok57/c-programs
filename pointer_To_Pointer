#include<stdio.h>
int main(){
    int x = 10;
    int *p;
    int **q;
    p = &x;
    q = &p;
    printf("x value is %d \n",x);
    printf("address of x is %p\n",&x);
    printf("The value of p is %p\n",p);
    printf("The address of p id %p\n",&p);
    printf("The value pointed by the pointer p is %d\n",*p);
    printf("The value of q is %p\n",q);
    printf("The address of q is %p\n",&q);
    printf("The value of x by using pointer to pointer i.e q is %d",**q);
    return 0;
}
